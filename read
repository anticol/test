javascript:(function(){const selector=".button.info-button";const h=prompt("Enter hour (0-23) CET:");const m=prompt("Enter minute (0-59) CET:");if(h!==null&&m!==null){alert("Auto-click scheduled for "+h.padStart(2,"0")+":"+m.padStart(2,"0")+" CET");function clickButton(){const btn=document.querySelector(selector);if(btn){btn.scrollIntoView({behavior:"smooth",block:"center"});btn.focus();btn.click();console.log("Button clicked at "+new Date().toLocaleTimeString())}else{console.log("Button not found at "+new Date().toLocaleTimeString())}}function waitForTarget(){const now=new Date();const target=new Date();target.setHours(parseInt(h,10),parseInt(m,10),0,0);const waitMs=target-now;setTimeout(clickButton,waitMs>0?waitMs:0)}waitForTarget();}})();
