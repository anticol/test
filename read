javascript:(function(){const selector=".button.info-button";function clickButton(){const btn=document.querySelector(selector);if(btn){btn.scrollIntoView({behavior:"smooth",block:"center"});btn.focus();btn.click();console.log("Button clicked at "+new Date().toLocaleTimeString());alert("Button clicked!")}else{console.log("Button not found at "+new Date().toLocaleTimeString())}}function waitForTarget(){const now=new Date();const target=new Date();target.setHours(1,0,0,0);const waitMs=target-now;setTimeout(clickButton,waitMs>0?waitMs:0)}waitForTarget();})();
